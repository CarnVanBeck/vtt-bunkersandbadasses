{
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "bracketSpacing": true,
    "endOfLine": "crlf",
    "printWidth": 120,
    "semi": true,
    "overrides": [
        {
            "files": "**/*.hbs",
            "options": {
                "parser": "angular"
            }
        }
    ]
}
