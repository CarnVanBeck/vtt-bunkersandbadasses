	viewBox="0 0 350 350" xmlns="http://www.w3.org/2000/svg"
>
	<path class="pentagon"
		d="m130,100 l0,100 l80,40 l80,-60 l-50,-100 z"	
	>
	</path>
	<path class="pentagon"
		d="m130,100 l0,100 l-60,20 l-30,-70 l50,-80 z"	
	>
	</path>
	<path class="pentagon"
		d="m130,100 l-40,-30 l60,-30 l70,10 l20,30 z"	
	>
	</path>
	<path class="pentagon"
		d="m130,200 l80,40 l-30,40 l-60,-0 l-50,-60 z"	
	>
	</path>
	<path class="pentagon"
		d="m210,240 l-30,40 l60,-30 l60,-60 l-10,-10 z"	
	>
	</path>
	<path class="pentagon"
		d="m300,190 l-10,-10 l-50,-100 l-20,-30 l50,50 z"	
	>
	</path>
	<text class="diceNumber"
		x="180" y="60" stroke="aliceblue"
		transform="rotate(26) translate(10,30)" >
		12
	</text>
</svg>