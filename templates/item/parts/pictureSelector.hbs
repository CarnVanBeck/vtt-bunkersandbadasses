<div class="{{name}}PicSelector pictureSelector picNoneDisplay" >
    {{#each choices as | choice | }}
        {{#if choice.icon}}
            <img class="{{../name}}PicOption PicOption {{../name}}PicOption{{choice.key}}"
                src="{{choice.icon}}" alt="{{choice.name}}" title="{{choice.name}}" data-key="{{choice.key}}"
            >
            </img>
        {{else}}
            <div data-key="{{choice.key}}">
                {{choice.name}}
            </div>
        {{/if}}
        {{#if ../descriptor}}
            <div class="typeText">
                {{choice.name}}
            </div>
        {{/if}}
    {{/each}}
</div>